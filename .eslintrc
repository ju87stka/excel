{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": "off",
    "arrow-parens": "off",

    "semi":"off",

    "accessor-pairs": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    'linebreak-style': ['error', 'windows']
  },
  "extends": ["eslint:recommended", "google"]
}